{
  "name": "react-16x-starter",
  "version": "1.0.0",
  "engines": {
    "node": ">=10.0.0 <=12.0.0"
  },
  "description": "React app",
  "license": "MIT",
  "author": "thanawat.petchsri@gmail.com",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config=config/webpack.prod.js",
    "build:docker": "docker build . -t react-16x-stater",
    "build:serve": "npm run build && npm run serve",
    "lint": "stylelint \"src/**/*.scss\"",
    "prettier": "prettier --check \"src/**/*.js\"  --check \"src/**/*.jsx\"",
    "prettier:write": "prettier --check \"src/**/*.js\"  --check \"src/**/*.jsx\" --write",
    "lint:fix": "stylelint \"src/**/*.scss\" --fix",
    "start": "node server.js",
    "start:dev": "cross-env NODE_ENV=development webpack serve --config=config/webpack.dev.js",
    "start:docker": "docker run -d -p 3000:80 react-16x-stater",
    "analyze": "webpack --config=config/webpack.prod.js --profile --json > webpack-bundle-analyzer/stats.json && webpack-bundle-analyzer webpack-bundle-analyzer/stats.json",
    "pretest": "npm run lint",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "serve": "serve dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-stage-0": "^7.8.3",
    "autoprefixer": "^10.2.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.20.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.2.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^11.0.1",
    "serve": "^11.3.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.11.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^4.1.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.24.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "workbox-webpack-plugin": "^6.1.1"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
